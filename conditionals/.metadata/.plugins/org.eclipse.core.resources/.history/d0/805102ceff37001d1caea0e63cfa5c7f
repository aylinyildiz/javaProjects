package miniProjectPrimeNumber;

public class Main {

	public static void main(String[] args) {
		int number = 2;

		for (int i = 2; i < number; i++) {
			if (number % i == 0) {
				System.out.println("Number is not prime number");
				break;
			}
			if (number % i != 0 && i == number / 2) {
				System.out.println("Prime number");
			}

		}
		
		//hocanÄ±n kodu
		

	}

}
