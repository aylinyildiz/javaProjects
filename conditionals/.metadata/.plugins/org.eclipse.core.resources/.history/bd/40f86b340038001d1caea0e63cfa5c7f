package miniProjectPrimeNumber;

public class Main {

	public static void main(String[] args) {
		int number = 1;
		boolean IsPrimeNum = true;

		for (int i = 2; i < number; i++) {
			if (number % i == 0) {
				IsPrimeNum = false;
			}
		}
		if (IsPrimeNum) {
			System.out.println("Prime number");
		} else {
			System.out.println("Number is not prime.");
		}

	}

}
